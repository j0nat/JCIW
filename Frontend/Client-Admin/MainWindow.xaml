<Window 
        x:Class="Client_Admin.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Client_Admin"
        mc:Ignorable="d" Background="#242424"
        Title="JCIW Admin Workspace" Height="450" Width="800">
    <Grid Name="MainGrid">
        <Frame Name="MainFrame" Source="Views\LoginView.xaml" NavigationUIVisibility="Hidden" 
                                   Width="{Binding ActualWidth, ElementName=MainGrid}" 
                                   Height="{Binding ActualHeight, ElementName=MainGrid}" />
    </Grid>
</Window>
